{
  "expo": {
    "name": "rippl",
    "slug": "rippl",
    "version": "1.0.0",
    "newArchEnabled": true,
    "scheme": "rippl",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-router"
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "$(Rippl) would like to access your camera.",
          "photosPermission": "$(Rippl) would like to access your gallery."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "$(Rippl) would like access to your photo gallery.",
          "savePhotosPermission": "$(Rippl) would like to save photos to your gallery."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7d9218d0-652f-4916-8a1e-4b5cdc247314"
      }
    },
    "owner": "frankblation"
  }
}
